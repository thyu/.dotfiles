[
    "#set here ~/.dotfiles/",
    "#set user_data {here}/user_data",
    "#set packageDir {user_data}/packages",
    "#mkdir {packageDir}",
    [
        "#mkdir {here}",
        "#gitupdate {here}"
    ],
    {
        "bash-powerline" : [
            "#set bash_powerline_url https://github.com/riobard/bash-powerline",
            "#gitupdate {packageDir}/bash-powerline {bash_powerline_url}",
            "#config {user_data}/.dotrc bash-powerline 'source {packageDir}/bash-powerline/bash-powerline.sh'"
        ],
        "config" : [
            "#config ~/.bashrc 'dotfiles-autorun' 'source ~/user_data/.dotrc'"
        ]
    },
    [
        "#dirsync {sync_src} {sync_dst} {ignoreList}"
    ]
]
